<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.accioma.telecosfacturamovil" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.InvoiceListActivity"
            android:label="@string/title_activity_invoice" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ProductListActivity"
            android:label="@string/title_activity_product_list" >
        </activity>
        <activity
            android:name=".activity.CustomerListActivity"
            android:label="@string/title_activity_customer_list" >
        </activity>
        <activity android:name=".activity.InvoiceFormActivity"
            android:label="@string/title_activity_invoice_form">
        </activity>
    </application>

</manifest>
